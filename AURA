                --///////////////////////////////////////TP  ROUPAS ////////////////////////////--


                EquipamentTP:AddButton({
                    Name = "TP Roupas",
                    Callback = function()
                        local destination = CFrame.new(-341.899414, 9.37642479, -649.204163, 0.0589874461, 2.73177143e-08, -0.99825871, 6.42189306e-08, 1, 3.11600843e-08, 0.99825871, -6.59451587e-08, 0.0589874461)
